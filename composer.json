    "keywords": ["laravel", "CKEditor4", "web editor", "douyasi", "wysiwyg", "image-upload"],
    "license": "MIT",
    "authors": [
        {
            "name": "raoyc",
            "email": "raoyc2009@gmail.com"
        }
